{
    "manifest_version": 2,
    "name": "   Notes Makers",
    "version": "1.0",
    "description": "This extension makes notes",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["eventPage.js"],
        "persistent": false
    },
    "permissions": [
        "storage",
        "contextMenus"
    ]

}